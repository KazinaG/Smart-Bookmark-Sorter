{
	"manifest_version": 3,
	"author": "Kazina",
	"homepage_url": "https://github.com/KazinaG/Smart-Bookmark-Sorter",
	"name": "Smart Bookmark Sorter",
	"short_name": "SBS",
	"description": "Frees you from organizing your bookmarks.",
	"version": "2.4.4",
	"icons": {
		"16": "src/extension/assets/icons/icon-16.png",
		"32": "src/extension/assets/icons/icon-32.png",
		"48": "src/extension/assets/icons/icon-48.png",
		"64": "src/extension/assets/icons/icon-64.png",
		"128": "src/extension/assets/icons/icon-128.png"
	},
	"action": {
		"default_icon": "src/extension/assets/icons/icon-128.png",
		"default_title": "Option",
		"default_popup": "src/extension/ui/options/index.html"
	},
	"background": {
		"service_worker": "src/extension/background/service-worker.js"
	},
	"permissions": [
		"bookmarks",
		"history",
		"storage"
	],
	"default_locale": "en"
}
